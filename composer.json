{
    "name": "ovidiuro/myfinance2",
    "type": "package",
    "description": "Laravel package for managing my finances",
    "keywords": [
        "laravel",
        "finance",
        "management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ovidiu Rosu",
            "email": "ovidiu.rosu90@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "jeremykenedy/laravel-blocker": "^5.0",
        "jeremykenedy/laravel-logger": "^11.1",
        "jeremykenedy/laravel-roles": "^11.5",
        "laravel/framework": "^12.0",
        "scheb/yahoo-finance-api": "^4.11.1",
        "symfony/process": "^7.0",
        "thiagoprz/eloquent-composite-key": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ovidiuro\\myfinance2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ovidiuro\\myfinance2\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ovidiuro\\myfinance2\\MyFinance2ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
